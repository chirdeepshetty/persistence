<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.tw.searchHistory.domain.SearchHistory" table="GROUP_SEARCH_HISTORY">
	<id name="id" column="id" type="long">
		<generator class="native"></generator>
	</id>
	<property name="ipAddress" column="ipAddress" type="string"></property>
	<property name="searchString" column="query" type="string"></property>
	<property name="formattedDateTime" column="querytime" type="string"></property>
</class>
</hibernate-mapping>